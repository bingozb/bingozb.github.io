<div class="copyright" >
  {% set current = date(Date.now(), "YYYY") %}
  Copyright Â© {% if theme.since and theme.since != current %} {{ theme.since }} - {% endif %}
  <span itemprop="copyrightYear">{{ current }}
  </span>
  <span class="author" itemprop="copyrightHolder">{{ config.author }}
  </span>
  All rights reserved.
</div>
